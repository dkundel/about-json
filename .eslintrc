{
  "parser": "typescript-eslint-parser",
  "plugins": ["typescript"],
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "rules": {
    "import/extension": 0,
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "es5"
      }
    ],
    "prefer-const": "error"
  }
}
